#!/usr/local/bin/zsh

# Color support
if [ "$TERM" = "xterm" ]
then
    export TERM="xterm-256color"
fi

# LinuxBrew
export LINUX_BREW_ROOT=/home/linuxbrew/.linuxbrew
export PATH="${LINUX_BREW_ROOT}/bin:${LINUX_BREW_ROOT}/sbin:$PATH"

#pkg-config
export PKG_CONFIG_PATH=/usr/lib64/pkgconfig

alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'
