
# Color support
if [ "$TERM" = "xterm" ]
then
    export TERM="xterm-256color"
fi

# alt pbcopy
alias pbcopy='xsel --clipboard --input'
alias pbpaste='xsel --clipboard --output'

# LinuxBrew
export LINUX_BREW_ROOT=/home/linuxbrew/.linuxbrew
export PATH="${LINUX_BREW_ROOT}/bin:${LINUX_BREW_ROOT}/sbin:$PATH"

#pkg-config
export PKG_CONFIG_PATH=/usr/lib64/pkgconfig

